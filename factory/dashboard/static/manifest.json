  "shortcuts": [
    {
      "name": "Nova Story",
      "short_name": "Story",
      "description": "Criar nova User Story",
      "url": "/?action=new-story",
      "icons": [{ "src": "/static/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Kanban Board",
      "short_name": "Kanban",
      "description": "Acessar Kanban Board",
      "url": "/?view=kanban",
      "icons": [{ "src": "/static/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "Ver dashboard de produtividade",
      "url": "/analytics",
      "icons": [{ "src": "/static/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Meu Trabalho",
      "short_name": "My Work",
