  ],

  "screenshots": [
    {
      "src": "/static/screenshots/kanban.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Kanban Board - Vista Desktop"
    },
    {
      "src": "/static/screenshots/mobile.png",
      "sizes": "375x812",
      "type": "image/png",
