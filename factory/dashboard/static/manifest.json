      "label": "Kanban Board - Vista Mobile"
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+fda",
      "url": "/handle-protocol?url=%s"
    }
  ],

  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "application/json", ".md", ".txt"]
        }
      ]
    }
  },

  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/json": [".json"],
        "text/markdown": [".md"],
        "text/plain": [".txt"]
      }
    }
  ],

  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },

  "handle_links": "preferred",

  "edge_side_panel": {
    "preferred_width": 480
  },

  "related_applications": [],

  "iarc_rating_id": ""
}
